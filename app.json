{
  "expo": {
    "name": "IOUSathi",
    "slug": "IOUSathi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow IOUSathi to access your photo",
          "cameraPermission": "Allow IOUSathi to access your camera"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.IOUSathi"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "scheme": "IOUSathi"
}
